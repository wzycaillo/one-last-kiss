#include<stdio.h>
#include<math.h>
#include<string.h>
void countRepeat(char s[]) {
	int cnt[256] = { 0 };
	for (int i = 0; s[i]; cnt[(unsigned char)s[i++]]++)
		for (int i = 0; i < 256; i++) {
			if (cnt[i] >= 2) {
				printf("%c:%d\n", i, cnt[i]);
			}
		}
}
void fun(int x, int y, int z) {
	z = x * x + y * y;
	printf("%d\n", z);
}
int f(int a) {
	return a % 2;
}
long l(int g) {
	switch (g)
	{
	case 0: return 0;
	case 1:
	case 2:return 0;
	}
	return (f(g-1)+f(g+1));
}
double max(double a, double b) {
	if (a > b) {
		return a;
	}
	else {
		return b;
	}
}
double fax(double x, int y) {
	int i;
	double z;
	for (i = 1, z = x; i < y; i++) {
		z = z * x;
	}
		return z;
	}
int  ciallo(int x, int y) {
	int a, i = 0;
	for (i = 0; i < 100; ++i) {
		if (a = (i % 5 == 0) && (i % 10 != 0) && (i % 3 == 0)) {
			printf("%d", i);
		}
			if (a = (i - 50 >= 0) && (i - 50 < 10) && (i % 3 == 0)) {
				printf("%d\n", i);
			}
		
	}
		return i;
	}
int cl(int x) {
	for (int i = 2; i < x; i++) {
		if (x % i == 0) {
			return 0;
		}
	}
			return 1;
		
	
}
check(int x, int y, int n) {
	if (x >= 0 && x <= n - 1 && y >= 0 && y <= n - 1) {
		return 0;
	}
	else {
		return 1;
	}
}
int digit(int n, int k) {
	if (k <= 0 || n <= 0) {
		return 0;
	}
	int m = 0;
	int a, b;
	int x = n;
	while (n != 0) {
		n = n / 10;
		m = m + 1;
	}
	if (k > m) {
		return 0;
	}
	a = pow(10, k);
	b = x % a / pow(10, k - 1);
	return b;
}
int* find_middle(int a[], int n) {
	if (n % 2 == 0) {
		int m = n / 2;
		return &a[m];
	}
	if (n % 2 != 0) {
		int m = (n - 1) / 2;
		return &a[m];
	}
	return 0;
}
int main()
{
	int a = 38;
	fun(7, 3, a);
	printf("%d\n",a);
	int s[8] = { 1,2,3,4,5,6 }, i, d = 0;
	for (int i = 0; f(s[i]); i++) {
		d += s[i];
		printf("%d\n", d);
	}
	long k;
	k = l(7);
	printf("%ld\n", k);
	double x, y;
	int a_1;
	scanf_s("%lf %lf", &x, &y);
	printf("%.8lf\n", max(x, y));
	printf("%lf\n", fax(2.0, 3));
	printf("%d\n", ciallo(0, 100));
	scanf_s("%d", &a_1);
	if (cl(a_1))
		printf("是素数\n");
	else
		printf("no是素数\n");
	int b_1, b_2,b_3,b_4
		,arr[6];
	scanf_s("%d %d", &b_1, &b_2);
	printf("%d\n",digit(b_1, b_2));
	for (int i = 0; i < 6; i++) {
		scanf_s("%d", &arr[i]);
	}
	int* mid_ptr = find_middle(arr, 6);
	printf("%d\n", *mid_ptr);
	
	char str[100];
	scanf_s("%s", str);
	countRepeat(str);
	return 0;
	
